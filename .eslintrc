{
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "rules": {
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-console": 0,
    "no-undef": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "comma-dangle": 0
  }
}
